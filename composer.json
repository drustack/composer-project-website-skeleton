{
    "authors": [
        {
            "email": "hswong3i@pantarei-design.com",
            "name": "Wong Hoi Sing Edison"
        }
    ],
    "config": {
        "allow-plugins": true,
        "discard-changes": true,
        "preferred-install": {
            "drustack/*": "source"
        },
        "process-timeout": 3600,
        "secure-http": false,
        "vendor-dir": "vendor"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "The 'DruStack Standard Edition' distribution",
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./"
            }
        },
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:bower-asset",
                "type:drupal-library",
                "type:npm-asset"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "profiles/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patches": {
            "drupal/advagg": {
                "https://drupal.org/node/3258389": "https://git.drupalcode.org/project/advagg/-/merge_requests/6.diff"
            },
            "drupal/core": {
                "https://drupal.org/node/2362643": "https://git.drupalcode.org/project/drupal/-/merge_requests/1522.diff",
                "https://drupal.org/node/2429321": "https://git.drupalcode.org/project/drupal/-/merge_requests/1521.diff",
                "https://drupal.org/node/2825860": "https://git.drupalcode.org/project/drupal/-/merge_requests/1519.diff",
                "https://drupal.org/node/2862702": "https://git.drupalcode.org/project/drupal/-/merge_requests/1518.diff",
                "https://drupal.org/node/2869592": "https://git.drupalcode.org/project/drupal/-/merge_requests/1517.diff",
                "https://drupal.org/node/2896169": "https://git.drupalcode.org/project/drupal/-/merge_requests/1887.diff",
                "https://drupal.org/node/2924653": "https://drupal.org/files/issues/2022-05-19/2924653-38.patch",
                "https://drupal.org/node/2946333": "https://drupal.org/files/issues/2022-07-25/2946333-288.patch",
                "https://drupal.org/node/2985882": "https://drupal.org/files/issues/2022-05-30/2985882-field-142.patch",
                "https://drupal.org/node/3008924": "https://drupal.org/files/issues/2022-07-11/3008924-33.patch",
                "https://drupal.org/node/3015152": "https://drupal.org/files/issues/2022-07-06/3015152-159.patch",
                "https://drupal.org/node/3025039": "https://git.drupalcode.org/project/drupal/-/merge_requests/1105.diff",
                "https://drupal.org/node/3180227": "https://drupal.org/files/issues/2022-04-20/3180227-28.patch",
                "https://drupal.org/node/3255637": "https://drupal.org/files/issues/2022-08-08/3255637-58.patch"
            }
        }
    },
    "homepage": "https://drupal.org/project/drustack",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "name": "drustack/website-skeleton",
    "prefer-stable": true,
    "repositories": {
        "asset": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "require": {
        "composer/installers": "~1.9",
        "cweagans/composer-patches": "~1.7",
        "drupal/admin_toolbar": "3.x-dev",
        "drupal/advagg": "4.x-dev",
        "drupal/config_split": "2.0.x-dev",
        "drupal/core-composer-scaffold": "~9.4.0",
        "drupal/core-project-message": "~9.4.0",
        "drupal/core-recommended": "~9.4.0",
        "drupal/core-vendor-hardening": "~9.4.0",
        "drupal/examples": "3.x-dev",
        "drupal/features": "3.x-dev",
        "drupal/module_filter": "3.x-dev",
        "drupal/pathauto": "~1.8",
        "drupal/token": "1.x-dev"
    },
    "require-dev": {
        "composer/composer": "~2.0",
        "drupal/core-dev": "~9.4.0",
        "drush/drush": "~11.0",
        "oomphinc/composer-installers-extender": "~2.0",
        "zaporylie/composer-drupal-optimizations": "~1.2"
    },
    "type": "project"
}
