    "config": {
        "allow-plugins": true,
        "discard-changes": true,
        "preferred-install": {
            "drustack/*": "source"
        },
        "process-timeout": 3600,
        "secure-http": false,
        "vendor-dir": "vendor"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "The 'DruStack Standard Edition' distribution",
